  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Signal.CANdy.Core\Signal.CANdy.Core.fsproj" />
  </ItemGroup>

</Project>
